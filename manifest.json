{
  "manifest_version": 3,
  "name": "PromptSavvy",
  "version": "1.0.0",
  "description": "Use and share the most powerful ChatGPT Prompts with PromptSavvy",
  "action": { "default_popup": "index.html" },
  "key": "",
  "background": {
    "service_worker": "src/background.ts",
    "type": "module"
  },
  "icons": {
    "128": "prompt-savvy-logo.png"
  },
  "content_scripts": [
    {
      "js": ["src/content.tsx"],
      "matches": ["https://*.chat.openai.com/*"]
    }
  ],
  "oauth2": {
    "client_id": "734028315606-b2np2glln8svj5pj6d8r26aevivm05si.apps.googleusercontent.com",
    "scopes": ["https://www.googleapis.com/auth/userinfo.email"]
  },
  "permissions": ["tabs", "bookmarks", "storage", "identity", "identity.email"],
  "web_accessible_resources": [
    {
      "resources": ["*.png"],
      "matches": ["https://*.chat.openai.com/*"]
    }
  ],
  "externally_connectable": {
    "matches": ["http://127.0.0.1:5173/*"]
  }
}

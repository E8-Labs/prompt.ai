{
  "name": "ChatgptProject",
  "version": "0.0.1",
  "manifest_version": 3,
  "key": "",
  "action": {
    "default_icon": "download.png",
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "matches": [
        "https://*.chat.openai.com/*"
      ],
      "js": [
        "jquery.js",
           "jquery_select.js",
        "popper.js",
        "bootstrap.js",
        "./Prompt_functions/render_public_prompts.js",
              "./screen_initialize_functions/side_menu.js",
              "./screen_initialize_functions/menu_items.js",
              "./screen_initialize_functions/initialize_main_screen_on_new_chat.js",
              "./screen_initialize_functions/initialize_main_screen_for_not_logged_in.js",
              "./clearInputs.js",
              "./screen_initialize_functions/extension_enabled.js",
              "./screen_initialize_functions/extension_disabled.js",
        "./Prompt_functions/fetch_categories.js",
        "./variables_declaration/api_data.js",
        "./variables_declaration/creating_category.js",
        "./variables_declaration/selectors_info.js",
        "./variables_declaration/filter.js",
        "./variables_declaration/pagination.js",
        "./variables_declaration/prompt_info.js",
        "./variables_declaration/lang_wrapper.js",
        "./variables_declaration/user_info.js",
        "./variables_declaration/our_submit_button.js",
      
        "./variables_declaration/images.js",
       
        "./Popup_screens/popup_screens_raw_data.js",
        "./Prompt_functions/creating_prompt.js",
        "./Prompt_functions/fetch_prompts.js",
        "./Filter_functions/main_filter.js",
        
        "./Prompt_functions/upvoting_public.js",
        "./Filter_functions/categories.js",
     
        "./Filter_functions/search_bar.js",
        "./Prompt_functions/upvoting_private.js",
        "content.js"
       
      ],
      "css": [
        "bootstrap.css",
        "styles.css"
      ]
    }
  ],
  "oauth2": {
    "client_id": "734028315606-b2np2glln8svj5pj6d8r26aevivm05si.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/userinfo.email"
    ]
  },
  "permissions": [
    "tabs",
    "bookmarks",
    "storage",
    "identity",
    "identity.email"
    
  ],

  "web_accessible_resources": [
    {
      "resources": [
        "*.png"
       
      ],
      "matches": [
        "https://*.chat.openai.com/*"
      ]
    }
  ]
}